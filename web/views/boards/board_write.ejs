  <body>
    <h1>글쓰기</h1>
    <form action="/board_write" method="post">
      <table border="1">
          <tr>
              <td>제목</td>
              <td><input type="text" name="post_title" /></td>
          </tr>
          <tr>
              <td>내용</td>
              <td><textarea name="post_content" cols="60" rows="20"></textarea></td>
          </tr>
          <div class="d4">
            <div class="d5"><button type="submit" class="button">글쓰기</button></div>
            <a href="/board_list" class="button">리스트로 돌아가기</a>
          </div>
      </table>
    </form>
  </body>
